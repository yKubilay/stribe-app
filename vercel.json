{
    "version": 2,
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html",
        "status": 200
      }
    ],
    "headers": [
      {
        "src": "/src/(.*)",
        "headers": [
          {
            "key": "Content-Type",
            "value": "application/javascript"
          }
        ]
      }
    ]
  }
  